extends ../layout/default

block sidebarMain
    div(class="sidebarMain")
        center
            h6(class="badge badge-success") SIDEBAR TITLE
        hr
        center
            a(href="/auth/google", class="badge badge-warning") Google Auth
            a(href="/auth/spotify", class="badge badge-warning") Spotify Auth

block contentMain
    div(class="contentMain")
        h6(class="badge badge-success")= title
        hr
        div(class="border border-warning rounded")
            p styling for flash message warnings

block panel1
    div(class="panel panel-default rounded", id="panel-one")
        div(class="panelTitle")
            h6 panel one
        div(class="panelBody")
            p placeholder

block panel2
    div(class="panel panel-default rounded", id="panel-one")
